{
  "name": "env-guardian-cli",
  "version": "1.0.0",
  "description": "Validate, document, and type-check your environment variables",
  "author": "Darshan Tarsariya ",
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Darshan1606/env-guardian-cli"
  },
  "homepage": "https://github.com/Darshan1606/env-guardian-cli#readme",
  "keywords": [
    "env",
    "environment",
    "validation",
    "typescript",
    "dotenv",
    "cli",
    "type-safe",
    "developer-tools"
  ],
  "bin": {
    "env-guardian-cli": "./dist/cli/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test:run",
    "local": "tsx src/cli/index.ts"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "tsup": "^8.1.0",
    "tsx": "^4.15.6",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
